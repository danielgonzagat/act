+    out: List[Dict[str, Any]] = []
+    for ex in exs:
+        if _domain_from_ctx_sig(str(ex.get("ctx_sig") or "")) != want:
+            continue
+        sig = str(ex.get("expected_sig") or "")
+        if not sig or sig in uniq:
